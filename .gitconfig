[user]
  name = Patrick Arlt
  email = patrick.arlt@gmail.com
[push]
  default = matching
[color]
  ui = auto
[alias]
  a = add
  b = branch
  c = commit
  d = diff
  dw = diff --word-diff
  ds = diff --staged
  dws = diff --word-diff --staged
  co = checkout
  r = remote
  s = status
  sup = branch --all --verbose
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  unstage = reset HEAD --
  done = branch --merged
  todo = branch --no-merged
  new = checkout -b
  trash = branch -d
  getall = fetch --all --prune
  pub = push origin
  sync = pull upstream
  upsync = pull upstream master
[credential]
  helper = osxkeychain
[mergetool]
  keepBackup = true
[core]
  excludesfile = ~/.global_ignore
  autocrlf = false
  ignorecase = false